    "version": "0.2.0",
    "configurations": [
        {
            "name": "run root",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-s",
                "ganepomutfstate",
            ]
        },
        {
            "name": "run env list",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-s",
                "ganepomutfstate",
                "env",
                "list"
            ]
        },
        {
            "name": "run env add",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-s",
                "ganepomutfstate",
                "env",
                "add",
                "--name",
                "Amazing List",
            ]
        },
        {
            "name": "run sub add",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-s",
                "ganepomutfstate",
                "sub",
                "add",
                "--name",
                "Amazing Sub",
                "--environment-id",
                "amazing-list",
                "--id",
                "0560a729-8178-41a5-adbc-638e087abc50"
            ]
        }
    ]
