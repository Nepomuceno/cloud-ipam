    "version": "0.2.0",
    "configurations": [
        {
            "name": "run root",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-s",
                "$TEST_STORAGE_ACCOUNT",
            ]
        },
        {
            "name": "run env list",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-s",
                "$TEST_STORAGE_ACCOUNT",
                "env",
                "list"
            ]
        },
        {
            "name": "run env add",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-s",
                "$TEST_STORAGE_ACCOUNT",
                "env",
                "add",
                "--name",
                "Amazing List",
            ]
        }
    ]
}